<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="https://github.com" />

  <remote  name="legacy"
           revision="10.x"
           fetch="https://github.com/Legacy-AOSP/"
           sync-j="8" />

  <!-- General -->
  <project path="build/make" name="android_build" groups="pdk" remote="legacy" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  
  <!-- External -->
  <project path="external/sonivox" name="android_external_sonivox" remote="legacy" />
  
  <!-- Frameworks -->
  <project path="frameworks/av" name="android_frameworks_av" remote="legacy" /> 
  <project path="frameworks/base" name="android_frameworks_base" remote="legacy" />
  <project path="frameworks/native" name="android_frameworks_native" remote="legacy" />
  <project path="frameworks/opt/net/wifi" name="android_frameworks_opt_net_wifi" remote="legacy" />
  <project path="frameworks/opt/telephony" name="android_frameworks_opt_telephony" remote="legacy" />
  
  <!-- Packages -->
  <project path="packages/apps/Bluetooth" name="android_packages_apps_Bluetooth" groups="pdk-fs" remote="legacy" />
  <project path="packages/apps/Contacts" name="android_packages_apps_Contacts" groups="pdk-fs" remote="legacy" />
  <project path="packages/apps/PackageInstaller" name="android_packages_apps_PackageInstaller" groups="pdk-fs" remote="legacy" />
  <project path="packages/apps/Settings" name="android_packages_apps_Settings" groups="pdk-fs" remote="legacy" />
  <project path="packages/services/Telephony" name="android_packages_services_Telephony" groups="pdk-fs" remote="legacy" />

  <!-- System -->
  <project path="system/bt" name="android_system_bt" remote="legacy" />
  <project path="system/core" name="android_system_core" remote="legacy" />
  <project path="system/sepolicy" name="android_system_sepolicy" remote="legacy" />

</manifest>
